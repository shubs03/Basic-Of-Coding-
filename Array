
#include <iostream>

using namespace std;

int main()
{
     int arr[4];
     
     arr[0]=1;
     arr[1]=3;
     arr[2]=5;
     arr[3]=7;
   
   cout<<"The Third Element is: "<<arr[3]<<endl;
   cout<<"Second Element is: "<<arr[2]<<endl;
   
}

/* output 
   The Third Element is: 7
   The Second Element is: 5  
                            */
